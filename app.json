{
  "expo": {
    "name": "Talkien",
    "description": "One app to rule all talks.",
    "slug": "Talkien",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/talkien_android.png",
    "splash": {
      "backgroundColor": "#ff602c"
    },
    "android": {
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/talkien_ios.png",
      "supportsTablet": true
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#ff602c"
    },
    "updates": {
      "enabled": false
    }
  }
}
